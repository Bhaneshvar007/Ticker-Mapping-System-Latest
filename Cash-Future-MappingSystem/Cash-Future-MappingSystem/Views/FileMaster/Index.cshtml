@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-12 p-0">
    <ul class="nav">
        <li class="nav-item">
            <a class="nav-link text-dark" href="javascript:void(0);">
                <strong>Statistics</strong>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="javascript:void(0);">File Master</a>
        </li>
    </ul>
    <hr class="m-0">
</div>

<div class="col-md-12 pt-3">
    <div class="mb-4">
        <button type="button" class="btn btn-success" id="exportSubmit">
            Export
        </button>
    </div>
</div>

<div style="overflow:auto">
    <table class="table table-hover table-bordered" id="fileMaster_table">
        <thead>
            <tr style="background-color:#d3d3d3">
                <th>File Id</th>
                <th>FileName</th>
                <th>Email</th>
                <th>Reference Type</th>
                <th>Total Record</th>
                <th>Uploded By</th>
                <th>Uploded Date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@section Scripts {
    <script src="~/Assets/FileMaster.js"></script>
}
